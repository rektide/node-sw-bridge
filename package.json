{
	"name": "node-sw-bridge",
	"type": "module",
	"version": "0.0.4",
	"description": "Expose a ServiceWorker from a converted node http.IncomingRequest and htpt.ServerResponse",
	"main": "build/node-sw-bridge.js",
	"module": "node-sw-bridge.js",
	"scripts": {
		"build": "mkdir -p build/idl build/ts && npm run build:ts && npm run build:idl",
		"build:idl": "./idl-converter.js build/idl idl/*.idl",
		"build:ts": "webidl2ts -i ./idl/service-workers.idl -o build/ts/service-workers.d.ts",
		"clean": "rm -rf build",
		"prepublish": "npm run build",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "https://archive.voodoowarez.com/node-sw-bridge"
	},
	"keywords": [],
	"author": "rektide de la faye",
	"license": "MIT",
	"dependencies": {
		"is-main": "^0.2.0",
		"node-web-streams": "^0.2.2",
		"web-streams-polyfill": "^1.3.2"
	},
	"devDependencies": {
		"@webref/idl": "^2.8.1",
		"mz": "^2.6.0",
		"quasilon": "^1.2.0",
		"webidl2": "^24.2.0",
		"webidl2ts": "^1.0.4"
	}
}
